;    #nightSiteswap    = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF> |       (F22 | user)*                                     ;
;                                                                                                                                                                                                 ;
;    #style            = global*| local | (true | user)*|  false                                                                                                                                  ;
;    #style°           =                                                   <name>                     | <inner style>*                                                                            ;
;    #FX               = global*| local |  true         | (false | user)*                                                                                                                         ;
;    #reverseStyle     = global*| local |  true         | (false | user)*                                                                                                                         ;
;                                                                                                                                                                                                 ;
;    #balls            = global*| local | (true | user)*|  false         | <trail length 0*..50>      |  user*                          |      trail                                              ;
;    #currentThrows    = global*| local |  true         | (false | user)*                                                                                                                         ;
;                                                                                                                                                                                                 ;
;    #juggler          = global*| local | (true | user)*|  false         |  phantom         |  hands &| arms &| body                    |   (visible | user)*                                     ;
;    #jugglerTrail     = global*| local |  true         | (false | user)*                                                                                                                         ;
;    #dayJuggler       = global*| local | (true | user)*|  false         |  phantom         |  hands &| arms &| body                    |   (visible | user)*                                     ;
;    #nightJuggler     = global*| local |  true         | (false | user)*| (phantom | user)*|  hands &| arms &| body                    |   (visible | user)*                                     ;
;    #juggler          = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF>                                                           ;
;    #dayJuggler       = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF> |       (444 | user)*                                     ;
;    #nightJuggler     = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF> |       (CCC | user)*                                     ;
;    #dayBackground    = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF> |       (FFF | user)*                                     ;
;    #nightBackground  = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF> |       (000 | user)*                                     ;
;    #background       = global*| local                                  | <3 hexa-numbers 000..FFF>  | <6 hexa-numbers 000000..FFFFFF>              | user *                                     ;
;                                                                                                                                                                                                 ;
;    #colors           = global*| local | (true | user)*|  false                                                                                                                                  ;
;    #colors°          =          local*                                 | <color sequence>                                             |         (7 | user)*                                     ;
;    #randomColors     = global*| local |  true         | (false | user)*                                                                                                                         ;
;    #alternateColors  = global*| local |  true         | (false | user)*| [/]<period 9..1>                                             | (catch | 0 | user)* | (count      | [/]T)               ;
;                                                                                                                                                                                                 ;
;    #defaults         = global*| local |  true         | (false | user)*|    <number 1..9>                                             |         (1 | user)*                                     ;
;    #relativeDefaults = global*| local |  true         | (false | user)*                                                                                                                         ;
;                                                                                                                                                                                                 ;
;    #strobe           = global*| local |  true         | (false | user)*| [/]<period 1..9>                                             |         (1 | user)* | (count      | [/]T) | (catch | 0) ;
;    #robot            = global*| local |  true         | (false | user)*| [/]<period 1..9>                                             |         (1 | user)* | (count      | [/]T) | (catch | 0) ;
;    #flash            = global*| local |  true         | (false | user)*| [/]<period 1..9>                                             |         (1 | user)* | (count      | [/]T) | (catch | 0) ;
