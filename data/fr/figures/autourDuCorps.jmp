;    #throwSounds        =  global*| local |  true          | (false | user)*                                                            ;
;    #metronome          =  global*| local |  true          | (false | user)*                                                            ;
;    #light              =  global*| local | (true | user)* |  false                                                                     ;
;                                                                                                                                        ;
;    #height             =  global | local*| <number  1..50 >                |             <float 0.01..0.50> |        (12 | user)*      ;
;    #speed              =  global*| local | (true | user)* |  false                                                                     ;
;    #speed              =  global*| local                                   |             <float  0.1..3.0 > |       (1.5 | user)*      ;
;    #dwell              =  global | local*| <number  1..99 >                |             <float 0.01..0.99> |        (70 | user)*      ;
;    #fluency            =  global*| local | (true | user)* |  false                                                                     ;
;    #fluency            =  global*| local |                                                 <number  1..99 > |        (99 | user)*      ;
;                                                                                                                                        ;
;    #warnings           =  global*| local | (true | user)* | false                                                                      ;
;    #errors             =  global*| local | (true | user)* | false                                                                      ;
;    #skill°             = (beginner | 0)  | (1    | user   | amateur)*      | (confirmed | 2) | (pro | 3)    |         (impossible | 4) ;
;    #mark°              =                    1..5 | 3*                                                                                  ;
