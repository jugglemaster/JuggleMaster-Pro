;    #dayJuggler         =  global*| local | (true  | user)*|  false                                                                     ;
;    #nightJuggler       =  global*| local |  true          | (false | user)*                                                            ;
;    #juggler            =  global*| local |        phantom                  |  hands &| arms &| head         |   (visible | user)*      ;
;    #dayJuggler         =  global*| local |        phantom                  |  hands &| arms &| head         |   (visible | user)*      ;
;    #nightJuggler       =  global*| local |       (phantom |  user)*        |  hands &| arms &| head         |   (visible | user)*      ;
;                                                                                                                                        ;
