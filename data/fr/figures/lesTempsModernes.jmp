;    #nightJugglerColor  =  global*| local |    <3 hexa-number 000..FFF>     | <6 hexa-number 000000..FFFFFF> |       (CCC | user)*      ;
;    #dayBackground      =  global*| local |    <3 hexa-number 000..FFF>     | <6 hexa-number 000000..FFFFFF> |       (FFF | user)*      ;
;    #nightBackground    =  global*| local |    <3 hexa-number 000..FFF>     | <6 hexa-number 000000..FFFFFF> |       (000 | user)*      ;
;    #background         =  global*| local |    <3 hexa-number 000..FFF>     | <6 hexa-number 000000..FFFFFF>              | user *      ;
;                                                                                                                                        ;
;    #colors             =  global*| local | (true  | user)*|  false                                                                     ;
;    #colors°            =           local*                                  | <color sequence>               |         (7 | user)*      ;
;    #randomColors       =  global*| local |  true          | (false | user)*                                                            ;
;    #alternateColors    =  global*| local |  true          | (false | user)*                                                            ;
;    #alternateColors    =  global*| local |                [/]<period 9..1> | (catch | 0 | user)* | (count   | [/]T)                    ;
;                                                                                                                                        ;
;    #defaults           =  global*| local |  true          | (false | user)*                                                            ;
;    #defaults           =  global*| local |  <number 1..9>                  |         (1 | user)*                                       ;
;    #randomDefaults     =  global*| local |  true          | (false | user)*                                                            ;
;                                                                                                                                        ;
;    #strobe             =  global*| local |  true                                                                                       ;
;    #strobe             =  global*| local |  [/]<period 1..9>               |         (1 | user)* | (count   | [/]T)      | (catch | 0) ;
